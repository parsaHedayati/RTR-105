    {
      if (a > b && b > c)
	{
	  printf ("%d\n", a);
	  printf ("%d\n", b);
	  printf("%d\n", c);

	}
	 if (a > c && c > b)
	{
	  printf ("%d\n", a);
	  printf ("%d\n", c);
	  printf("%d\n", b);
	} 
      if (b > a && a > c)
